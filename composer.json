{
    "name": "temistocle1998/laravel-model-tracker",
    "description": "A Laravel package for tracking model changes, including old values, new values, and the user responsible for the changes",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mamadou lamine BÃ¨ye",
            "email": "laminebeye007@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tracker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tracker\\TrackerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tracker\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
